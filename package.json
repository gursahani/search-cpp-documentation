{
	"name": "library-documentation-cpp",
	"description": "Search Documentation, docs, devdocs, cpp",
	"version": "0.0.5",
	"publisher": "kunalg",
	"engines": {
		"vscode": "^1.31.0"
	},
	"icon":"img/cpp_logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gursahani/search-cpp-documentation.git"
	  },
	"categories": ["Other", "Programming Languages"], 
	
	"keywords":["search", "documentation", "docs", "devdocs", "cpp"],
	"activationEvents": [
		"onCommand:docs.start",
		"onWebviewPanel:docs",
		"onLanguage:cpp"
		
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "docs.start",
				"title": "cpp docs",
				"category": "search"
			}
		],
		"keybindings": [{
			"command": "docs.start",
			"key": "ctrl+shift+A",
			"mac": "cmd+shift+A",
			"when": "editorTextFocus"
		}]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"vscode": "^1.1.29",
		"@types/node": "^10.5.2",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2"
	}
}
